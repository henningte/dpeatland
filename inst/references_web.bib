 doi = {10.1002/ece3.4134},
 file = {Fisher, Wilson et al. 2018 - A simple function for full-subsets:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Fisher, Wilson et al. 2018 - A simple function for full-subsets.pdf:pdf}
