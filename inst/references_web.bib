 journal = {{Applied spectroscopy}},
 doi = {10.1366/10-06114},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/21396181},
 file = {Ryu, Noda et al. 2011 - Two-dimensional correlation analysis and waterfall:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Ryu, Noda et al. 2011 - Two-dimensional correlation analysis and waterfall.pdf:pdf}
}


