 number = {1-3},
 journal = {{The Science of the total environment}},
 doi = {10.1016/j.scitotenv.2005.06.002},
 file = {Blodau, Basiliko et al. 2006 - The fate of experimentally deposited:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Blodau, Basiliko et al. 2006 - The fate of experimentally deposited.pdf:pdf}
