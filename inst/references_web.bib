 doi = {10.1021/es502502n},
 file = {Chen, Qian et al. 2014 - Two-dimensional correlation spectroscopic analysis:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Chen, Qian et al. 2014 - Two-dimensional correlation spectroscopic analysis.pdf:pdf}
