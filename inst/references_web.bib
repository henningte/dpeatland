 pages = {314--322},
 pagination = {page},
 volume = {571},
 journal = {{The Science of the total environment}},
 doi = {10.1016/j.scitotenv.2016.07.152},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/27487447},
 file = {Manninen, Kivimäki et al. 2016 - Nitrogen deposition does not enhance:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Manninen, Kivimäki et al. 2016 - Nitrogen deposition does not enhance.pdf:pdf}
