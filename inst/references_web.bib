 journal = {{ISPRS International Journal of Geo-Information}},
 doi = {10.3390/ijgi7080294},
 file = {Chabot, Dillon et al. 2018 - An Object-Based Image Analysis Workflow:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Chabot, Dillon et al. 2018 - An Object-Based Image Analysis Workflow.pdf:pdf}
