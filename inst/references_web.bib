 number = {3},
 journal = {{Canadian journal of microbiology}},
 doi = {10.1139/W10-116},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/21358764},
 file = {Jassey2011_CanJMicrob:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Jassey2011_CanJMicrob.pdf:pdf}
}


