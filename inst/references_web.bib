 file = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4833502},
 file = {Bengtsson, Granath et al. 2016 - Photosynthesis:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Bengtsson, Granath et al. 2016 - Photosynthesis.pdf:pdf;bengtsson_etal_2016_traits_meta_v2:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\bengtsson_etal_2016_traits_meta_v2.csv:csv;bengtsson_etal_2016_traits:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\bengtsson_etal_2016_traits.csv:csv}
