 file = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6143518},
 file = {Kang, Kwon et al. 2018 - Biologically driven DOC release:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Kang, Kwon et al. 2018 - Biologically driven DOC release.pdf:pdf}
}


