 pages = {12599--12622},
 pagination = {page},
 volume = {20},
 number = {7},
 journal = {{Molecules (Basel, Switzerland)}},
 doi = {10.3390/molecules200712599},
 file = {Baldassarre, Li et al. 2015 - Simultaneous Fitting of Absorption Spectra:G\:\\projects\\fyi\\literature\\fyi\\Citavi Attachments\\Baldassarre, Li et al. 2015 - Simultaneous Fitting of Absorption Spectra.pdf:pdf}
